<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.consensus.prototypes.mongoose.storm.model.GpaScore"
		table="STUDENT">
		<id name="studentOId" type="int">
			<column name="studentOId" />
			<generator class="identity" />
		</id>
		<property name="studentId" type="java.lang.String">
			<column name="studentId" />
		</property>
		<property name="GpaScore" type="double">
			<column name="gpaScore" length="10" />
		</property>
		<property name="Prestige" type="double">
			<column name="prestige" length="10" />
		</property>
	</class>
</hibernate-mapping>
