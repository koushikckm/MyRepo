		<interceptors>
			 <interceptor name="session" class="com.mazda.common.interceptors.SessionCheckInterceptor" />
			<interceptor name="gzip" class="com.mazda.common.interceptors.GZIPInterceptor" />
			<interceptor name="cache" class="com.mazda.common.interceptors.CachingHeaderInterceptor" />
			<interceptor name="common" class="com.mazda.common.interceptors.CommonInterceptor" />
			<interceptor-stack name="commonValidationStack">
				<interceptor-ref name="common" />
				<interceptor-ref name="gzip" />
				<interceptor-ref name="cache" />
				<interceptor-ref name="session" />
				<interceptor-ref name="defaultStack">
					<param name="exception.logEnabled">true</param>
					<param name="exception.logLevel">ERROR</param>
				</interceptor-ref>
			</interceptor-stack>
			<interceptor-stack name="welcomeStack">
				<interceptor-ref name="common" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>

		</interceptors>

		<default-interceptor-ref name="commonValidationStack" />
		<global-results>
			<result name="myErrorHandler" type="redirectAction">
				<param name="actionName">errorHandler</param>
				<param name="exception">exception</param>
			</result>
			<result name="sessionexpired" type="redirectAction">
				<param name="actionName">welcome</param>
				<param name="exception">session_expired</param>
			</result>
		</global-results>

		<global-exception-mappings>
			<exception-mapping exception="java.lang.Exception"
				result="myErrorHandler" />
		</global-exception-mappings>
		
